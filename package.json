{
  "name": "ng9-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:ci": "ng test -c=ci",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e -c=ci",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:scss:fix": "stylelint 'src/**/*.scss' --syntax scss --fix",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "prettier:ts:check": "prettier --check ./**/*.ts",
    "prettier:ts:write": "prettier --write ./**/*.ts",
    "prettier:js:check": "prettier --check ./**/*.js",
    "prettier:js:write": "prettier --write ./**/*.js",
    "prettier:scss:check": "prettier --check ./**/*.scss",
    "prettier:scss:write": "prettier --write ./**/*.scss",
    "prettier:json:check": "prettier --check ./**/*.{json,*rc}",
    "prettier:json:write": "prettier --write ./**/*.{json,*rc}",
    "prettier:check": "npm run prettier:ts:check && npm run prettier:js:check && npm run prettier:scss:check && npm run prettier:json:check",
    "prettier:write": "npm run prettier:ts:write && npm run prettier:js:write && npm run prettier:scss:write && npm run prettier:json:write",
    "precommit": "npm run prettier:write && npm run lint:js:fix && npm run lint && npm run test:ci",
    "prepush": "npm run test:ci",
    "postinstall": "webdriver-manager update --standalone false --gecko false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0",
    "@angular/common": "9.0.0",
    "@angular/compiler": "9.0.0",
    "@angular/core": "9.0.0",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "9.0.0",
    "@angular/platform-browser": "9.0.0",
    "@angular/platform-browser-dynamic": "9.0.0",
    "@angular/router": "9.0.0",
    "rxjs": "6.5.4",
    "tslib": "1.10.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.1",
    "@angular/cli": "9.0.1",
    "@angular/compiler-cli": "9.0.0",
    "@angular/language-service": "9.0.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/jasmine": "3.5.3",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.26",
    "@types/puppeteer": "2.0.0",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "codelyzer": "5.2.1",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.2.1",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.5.2",
    "lint-staged": "10.0.7",
    "prettier": "1.19.1",
    "protractor": "5.4.3",
    "puppeteer": "2.1.1",
    "stylelint": "13.0.0",
    "stylelint-config-recommended": "3.0.0",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.7.5"
  }
}
